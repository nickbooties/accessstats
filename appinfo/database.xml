<?xml version="1.0" encoding="iso-8859-1"?>
<database>
 <name>*dbname*</name>
 <create>true</create>
 <overwrite>false</overwrite>
 <charset>utf8</charset>
 <table>
  <name>*dbprefix*accessstats_items</name>
  <declaration>
   <field>
    <name>id</name>
    <type>integer</type>
    <default>0</default>
    <notnull>true</notnull>
    <autoincrement>1</autoincrement>
    <length>4</length>
   </field>
   <field>
    <name>username</name>
    <type>text</type>
    <notnull>false</notnull>
    <length>100</length>
   </field>
   <field>
    <name>path</name>
    <type>text</type>
    <notnull>false</notnull>
    <length>1000</length>
   </field>
   <field>
    <name>time</name>
    <type>timestamp</type>
    <notnull>true</notnull>
   </field>
  </declaration>
 </table>
</database>
